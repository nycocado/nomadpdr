{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"app/**/*.{ts,tsx,js,jsx}\"",
    "docker:up": "docker compose -f docker-compose.yml up --build",
    "docker:up:bg": "docker compose -f docker-compose.yml up -d --build",
    "docker:down": "docker compose -f docker-compose.yml down -v",
    "docker:restart": "docker compose -f docker-compose.yml restart",
    "docker:rebuild": "docker compose -f docker-compose.yml down -v && docker compose -f docker-compose.yml up --build",
    "docker:rebuild:bg": "docker compose -f docker-compose.yml down -v && docker compose -f docker-compose.yml up -d --build",
    "docker:logs": "docker compose -f docker-compose.yml logs -f",
    "docker:exec:web": "docker compose -f docker-compose.yml exec web sh",
    "docker:clean": "docker compose -f docker-compose.yml down -v && docker system prune -f",
    "docker:health": "docker compose -f docker-compose.yml ps"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.7.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.3",
    "@react-email/render": "^2.0.1",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "negotiator": "^1.0.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "resend": "^6.6.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
